{
  "$id": "ImageStats",
  "$schema": "http://json-schema.org/draft-07/schema",
  "type": "object",
  "properties": {
    "count": {
      "type": "number"
    },
    "rows": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "count": {
            "type": "number"
          },
          "state": {
            "$ref": "ImageState#"
          }
        },
        "additionalProperties": false,
        "required": [
          "count",
          "state"
        ]
      }
    }
  },
  "additionalProperties": false,
  "required": [
    "count",
    "rows"
  ]
}
