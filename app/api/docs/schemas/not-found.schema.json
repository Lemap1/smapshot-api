{
  "$id": "NotFoundResponse",
  "$schema": "http://json-schema.org/draft-07/schema",
  "type": "object",
  "description": "A response body indicating that the resource was not found.",
  "properties": {
    "$$merge": {
      "file": "./generic-http-problem-details.res.schema.json",
      "pointer": "/properties"
    },
    "type": {
      "example": "https://httpstatuses.com/404"
    },
    "title": {
      "example": "Not Found"
    },
    "status": {
      "example": 404
    }
  },
  "additionalProperties": false,
  "required": [
    "detail",
    "status",
    "title",
    "type"
  ]
}
