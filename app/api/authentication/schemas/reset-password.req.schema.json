{
  "$id": "ResetPasswordRequest",
  "$schema": "http://json-schema.org/draft-07/schema",
  "type": "object",
  "properties": {
    "token": {
      "type": "string"
    },
    "password": {
      "type": "string",
      "minLength": 6
    }
  },
  "additionalProperties": false,
  "required": [
    "token",
    "password"
  ]
}
